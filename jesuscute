
local player = game.Players.autofarmtester3 -- my name


player.Chatted:Connect(function(cmd)
    if cmd == "/e ;bring" then
    	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart
    	elseif cmd == "/e ;freeze" then
    	game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    	elseif cmd == "/e ;unfreeze" then
    	game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    	elseif cmd == "/e ;kick" then
    	game.Players.LocalPlayer:Kick('You have been kicked by an admin')
    	elseif cmd == "/e ;reset" then
    	game.Players.LocalPlayer.Character.Head:Destroy()
    	elseif cmd == "/e ;breakchar" then
    	game.Players.LocalPlayer.Character.Humanoid:Destroy()
    	--
    	elseif cmd == ";bring" then
    	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = player.Character.HumanoidRootPart
    	elseif cmd == ";freeze" then
    	game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
    	elseif cmd == ";unfreeze" then
    	game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
    	elseif cmd == ";kick" then
    	game.Players.LocalPlayer:Kick('You have been kicked by an admin')
    	elseif cmd == ";reset" then
    	game.Players.LocalPlayer.Character.Head:Destroy()
    	elseif cmd == ";breakchar" then
    	game.Players.LocalPlayer.Character.Humanoid:Destroy()
    	
	end
end)
